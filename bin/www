#!/usr/bin/env node
var express = require('express');
var app = express();

const api = require('../api/index');
app.use('/api', api);
const port = process.env.PORT || 3000;
app.set('port', port);

app.listen(port, function() {
  console.log('HUGE NavExercise application is available at http://localhost:' + port);
});